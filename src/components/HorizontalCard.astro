---
import type {HorizontalCardProps} from '../interfaces/interface';

const {tittle, description} = Astro.props as HorizontalCardProps;
---


<section class="bg-white rounded-3xl px-4 py-4 max-w-7xl mx-5 my-5 place-self-center-safe">
        <h2 class="text-2xl font-bold mb-2">
            <p>{tittle}</p>
        </h2>
        <h2 class="">
            <span>
                <p>{description}</p>
            </span>
        </h2>
</section>